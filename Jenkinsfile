pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        echo 'a'
        writeFile(file: 'd:\\1.txt', text: 'Artifact https://wiki.jenkins.io/display/JENKINS/Copy+Artifact+Plugin  https://dotblogs.com.tw/stanley14/2018/06/19/jenkins_copyartifact_plugin  https://medium.com/@lvthillo/send-slack-notifications-in-jenkins-pipelines-using-a-shared-library-873ca876f72c  https://jenkins.io/doc/pipeline/examples/  Pipeline {', encoding: 'utf8')
      }
    }
  }
}